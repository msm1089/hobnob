{
  "name": "hobnob",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist --copy-files",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon -w src --exec yarn run serve",
    "watch-test": "npx dotenv -e envs/test.env yarn run watch",
    "es-start": "./batchfiles/es-start.bat",
    "es-stop": "./batchfiles/es-stop.bat",
    "es-init": "./batchfiles/es-init.bat",
    "killserver": "./batchfiles/killserver.bat",
    "es-checkisup": "curl \"http://localhost:9200/?pretty\"",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "precommit": "yarn run lint",
    "test:e2e": "dotenv -e envs/test.env -e envs/.env ./scripts/e2e.test.sh",
    "serve": "yarn run build && dotenv -e envs/test.env -e envs/.env && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/msm1089/hobnob.git"
  },
  "author": "Mark McPherson",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "cucumber": "^5.0.2",
    "dotenv-cli": "^1.4.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.0",
    "husky": "^1.1.2",
    "node-cmd": "^3.0.0",
    "nodemon": "^1.18.5",
    "prettier": "^1.15.1",
    "rimraf": "^2.6.2",
    "superagent": "^4.0.0-beta.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "ajv": "^6.5.5",
    "body-parser": "^1.18.3",
    "elasticsearch": "^15.2.0",
    "express": "^4.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  }
}
