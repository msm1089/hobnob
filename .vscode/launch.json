{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node",
      "program": "${file}",
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch index.js",
      "program": "${workspaceRoot}/src/index.js",
      "protocol": "inspector",
      "envFile": "${workspaceFolder}/envs/.env",
      "env": {
        "BABEL_ENV": "debug",
        "ELASTICSEARCH_INDEX": "test",
        "SERVER_PORT": "8888"
      },
      "outFiles": ["${workspaceRoot}/dist/**/*.js"]
    },
    {
      "name": "Node with Experimental Modules",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--experimental-modules"],
      "program": "${file}",
      "protocol": "inspector"
    }
  ],
  "compounds": []
}
