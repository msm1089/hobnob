{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/src/index.js",
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
      "outFiles": ["${workspaceRoot}/dist/**/*.js"],
      "envFile": "${workspaceFolder}/envs/.env",
      "env": {
        "BABEL_ENV": "debug",
        "ELASTICSEARCH_INDEX": "test",
        "SERVER_PORT": "8888"
      },
      "preLaunchTask": "build"
    }
  ]
}
